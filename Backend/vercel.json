{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],

      "routes": [
        {
          "src": "/api/uploadinput",
          "methods": ["POST"],
          "dest": "<YOUR_EXPRESS_SERVER_FILE>"
        },
        {
          "src": "/api/uploaddataset",
          "methods": ["POST"],
          "dest": "<YOUR_EXPRESS_SERVER_FILE>"
        },
        {
          "src": "/api/getText",
          "methods": ["GET"],
          "dest": "<YOUR_EXPRESS_SERVER_FILE>"
        },
        {
          "src": "/api/analyze",
          "methods": ["GET"],
          "dest": "<YOUR_EXPRESS_SERVER_FILE>"
        },
        {
          "src": "/api/generateText",
          "methods": ["POST"],
          "dest": "<YOUR_EXPRESS_SERVER_FILE>"
        }
      ]
  }
    
  
  